{
    "name": "naggadim-dev/laravel-max-bot",
    "description": "MAX messenger bot framework for Laravel",
    "version": "0.1.2",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "naggadim",
            "email": "naggadim@vivladi.net"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "NaggaDIM\\LaravelMaxBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NaggaDIM\\LaravelMaxBot\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NaggaDIM\\LaravelMaxBot\\Laravel\\Providers\\LaravelMaxBotServiceProvider"
            ]
        }
    }
}